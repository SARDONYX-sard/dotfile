{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#21c7a8",
          "foreground": "#011627",
          "leading_diamond": "╭─",
          "properties": {
            "postfix": "  ",
            "windows": ""
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "os"
        },
        {
          "background": "#ef5350",
          "foreground": "#ffeb95",
          "leading_diamond": "<transparent,#ef5350></>",
          "properties": {
            "root_icon": " "
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "root"
        },
        {
          "background": "#70b8d4",
          "foreground": "#011627",
          "leading_diamond": "<transparent,#82AAFF></>",
          "properties": {
            "enable_hyperlink": true,
            "prefix": "  ",
            "style": "full"
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "path"
        },
        {
          "background": "#22da6e",
          // "background": "#e0d756",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#e7e077{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
            "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
            "{{ if gt .Behind 0 }}#C792EA{{ end }}"
          ],
          "foreground": "#011627",
          "powerline_symbol": "",
          "properties": {
            "branch_icon": " ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350>  {{ .Staging.String }}</>{{ end }}"
          },
          "style": "powerline",
          "type": "git"
        },
        {
          "background": "#575656",
          "foreground": "#d6deeb",
          "leading_diamond": "<transparent,#575656></>",
          "properties": {
            "postfix": " ",
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "executiontime"
        },
        {
          "background": "#234d70",
          "foreground": "#d6deeb",
          "leading_diamond": " ",
          "properties": {
            "postfix": " ",
            "prefix": "   ",
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#303030",
          "foreground": "#3C873A",
          "leading_diamond": " ",
          "properties": {
            "display_package_manager": true,
            "npm_icon": " <#cc3a3a></> ",
            "postfix": "",
            "prefix": " ",
            "yarn_icon": " <#348cba></>"
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "node"
        },
        {
          "background": "#306998",
          "foreground": "#FFE873",
          "leading_diamond": " ",
          "properties": {
            "postfix": "",
            "prefix": " "
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "python"
        },
        {
          "background": "#0e8ac8",
          "foreground": "#ec2729",
          "leading_diamond": " ",
          "properties": {
            "postfix": "",
            "prefix": " "
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "java"
        },
        {
          "background": "#0e0e0e",
          "foreground": "#0d6da8",
          "leading_diamond": " ",
          "properties": {
            "postfix": "",
            "prefix": " "
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "dotnet"
        },
        {
          "background": "#ffffff",
          "foreground": "#06aad5",
          "leading_diamond": " ",
          "properties": {
            "postfix": "",
            "prefix": "ﳑ "
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "go"
        },
        {
          "background": "#f3f0ec",
          "foreground": "#925837",
          "leading_diamond": " ",
          "properties": {
            "postfix": "",
            "prefix": " "
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "rust"
        },
        {
          "background": "#e1e8e9",
          "foreground": "#055b9c",
          "leading_diamond": " ",
          "properties": {
            "postfix": "",
            "prefix": " "
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "dart"
        },
        {
          "background": "#ffffff",
          "foreground": "#ce092f",
          "leading_diamond": " ",
          "properties": {
            "postfix": "",
            "prefix": " "
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "angular"
        },
        {
          "background": "#945bb3",
          "foreground": "#359a25",
          "leading_diamond": " ",
          "properties": {
            "postfix": "",
            "prefix": "<#ca3c34></> "
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "julia"
        },
        {
          "background": "#ffffff",
          "foreground": "#9c1006",
          "leading_diamond": " ",
          "properties": {
            "postfix": "",
            "prefix": " "
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "ruby"
        },
        {
          "background": "#ffffff",
          "foreground": "#5398c2",
          "leading_diamond": " ",
          "properties": {
            "postfix": "",
            "prefix": "<#f5bf45></> "
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "azfunc"
        },
        {
          "background": "#565656",
          "foreground": "#faa029",
          "leading_diamond": " ",
          "properties": {
            "postfix": "",
            "prefix": " ",
            "template": "{{.Profile}}{{if .Region}}@{{.Region}}{{end}}"
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "aws"
        },
        {
          "background": "#316ce4",
          "foreground": "#ffffff",
          "leading_diamond": " ",
          "properties": {
            "postfix": "",
            "prefix": "ﴱ ",
            "template": "{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}"
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#21c7a8",
          "properties": {
            "postfix": "",
            "prefix": "",
            "text": "╰─"
          },
          "style": "plain",
          "type": "text"
        },
        {
          "background": "#1DB954",
          "foreground": "#011627",
          "leading_diamond": "",
          "properties": {
            "playing_icon": " ",
            "track_separator": " ~ "
          },
          "style": "diamond",
          "trailing_diamond": " ",
          "type": "spotify"
        },
        {
          "foreground": "#22da6e",
          "foreground_templates": ["{{ if gt .Code 0 }}#ef5350{{ end }}"],
          "properties": {
            "always_enabled": true,
            "prefix": "",
            "template": ""
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": false,
  "osc99": true
}
